{
  "name": "gcharts",
  "version": "1.0.0",
  "description": "原生实现一个了类 echarts 的柱状图",
  "main": "index.js",
  "dependencies": {
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "husky": "^1.3.1",
    "less": "^3.9.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
	"parcel": "^1.11.0"
  },
  "scripts": {
    "dev": "parcel index.html --open",
    "build": "rimraf dist && parcel build index.html --no-source-maps --no-cache",
    "cdBuild": "rimraf dist && parcel build index.html --no-source-maps --no-cache --public-url /gcharts",
    "lint": "yarn _typeCheck && yarn _tsLint",
    "_typeCheck": "tsc --project tsconfig.json --noEmit",
    "_tsLint": "tslint -p ./tsconfig.json ./src/**/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },

  "repository": {
    "type": "git",
    "url": "git+https://github.com/guzhongren/gcharts.git"
  },
